{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": [
        "check-format",
        "require-const-for-all-caps"
      ]
    },
    "interface-name": [
      true,
      "never-prefix"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ]
  },
  "rulesDirectory": []
}
